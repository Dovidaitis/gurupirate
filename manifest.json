{
    "manifest_version": 3,
    "name": "gurupirate",
    "version": "0.2.2",
  
    "description": "Disable annoying paywalls on GuruFocus",
    "content_scripts": [
        {
            "matches": ["https://www.gurufocus.com/stock/*"],
            "js": ["main.js"],
            "inject_js":{
                "run_at": "document_end"
                }
        }
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "icons":
        {
            "64":"./assets/logo.png"
        }
    
    
}